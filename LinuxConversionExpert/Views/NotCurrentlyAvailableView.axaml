<UserControl xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="450"
             x:Class="LinuxConversionExpert.Views.NotCurrentlyAvailableView">
    <StackPanel Orientation="Vertical" Spacing="50" Margin="50" VerticalAlignment="Center" HorizontalAlignment="Center">
        <TextBlock FontSize="20" FontWeight="Bold">This functionality is currently unavailable and will be made available in a future update</TextBlock>
        <Button FontSize="20" VerticalAlignment="Stretch" HorizontalAlignment="Stretch" HorizontalContentAlignment="Center" Click="Back_OnClick">Back</Button>
    </StackPanel>
</UserControl>
